n = int(input())
N = [int(input()) for i in range(n)]

def process(num):
    s = 0
    for g in range(1, num):
        if not num % g:
            s += g
    if s == num:
        typ = "perfect"
    elif s > num:
        typ = "abundant"
    else: typ = "deficient"
    print(f"{num} is a {typ} number")
